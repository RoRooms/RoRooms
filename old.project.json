{
  "name": "RoRooms",
  
  "tree": {
    "$className": "DataModel",
    
    "ServerStorage": {
      "$ignoreUnknownInstances": true,
      "RoRoomsSource": {
        "$className": "Folder",
        "Shared": {
          "$path": "src/Shared",
          "Packages": {
            "$path": "Packages"
          },
          "ExtPackages": {
            "$path": "ExtPackages"
          }
        },
        "Storage": {
          "$path": "src/Storage",
          "ExtPackages": {
            "$path": "ExtServerPackages"
          }
        },
        "Server": {
          "$path": "src/Server"
        },
        "Client": {
          "$path": "src/Client"
        }
      }
    },
    "ReplicatedStorage": {
      "$ignoreUnknownInstances": true,
      "RoRooms": {
        "$path": "src/API"
      }
    }
  }
}